&lt;h2 id="-by-day">&amp;hellip; by day&lt;/h2>
&lt;canvas id="visitorsChart" width="400" height="200">&lt;/canvas>
&lt;script src="https://cdn.jsdelivr.net/npm/chart.js">&lt;/script>
&lt;script>
document.addEventListener('DOMContentLoaded', function() {
var dailyData = [{"date":"2025-02-17","users":213},{"date":"2025-02-04","users":202},{"date":"2025-02-12","users":201},{"date":"2025-02-19","users":149},{"date":"2025-02-27","users":148},{"date":"2025-02-05","users":147},{"date":"2025-02-03","users":146},{"date":"2025-02-18","users":143},{"date":"2025-02-13","users":142},{"date":"2025-02-21","users":134},{"date":"2025-02-28","users":128},{"date":"2025-02-11","users":124},{"date":"2025-02-14","users":123},{"date":"2025-02-24","users":123},{"date":"2025-02-25","users":116},{"date":"2025-02-06","users":112},{"date":"2025-02-20","users":112},{"date":"2025-02-26","users":103},{"date":"2025-02-07","users":101},{"date":"2025-02-10","users":101},{"date":"2025-03-03","users":95},{"date":"2025-02-16","users":89},{"date":"2025-02-15","users":79},{"date":"2025-02-23","users":69},{"date":"2025-02-22","users":64},{"date":"2025-03-04","users":64},{"date":"2025-03-01","users":60},{"date":"2025-03-02","users":60},{"date":"2025-02-09","users":59},{"date":"2025-02-08","users":38},{"date":"2025-03-05","users":1}];
if (dailyData &amp;&amp; dailyData.length) {
dailyData.sort((a, b) => new Date(a.date) - new Date(b.date));
dailyData.pop();
var labels = dailyData.map(function(entry) { return entry.date; });
var dataPoints = dailyData.map(function(entry) { return entry.users; });
var ctx = document.getElementById('visitorsChart').getContext('2d');
new Chart(ctx, {
type: 'line',
data: {
labels: labels,
datasets: [{
label: 'Daily Visitors',
data: dataPoints,
borderColor: 'rgba(75, 192, 192, 1)',
backgroundColor: 'rgba(75, 192, 192, 0.2)',
fill: true
}]
},
options: {
responsive: true,
scales: {
x: {
title: { display: true, text: 'Date' }
},
y: {
title: { display: true, text: 'Users' }
}
}
}
});
} else {
console.warn('No daily visitor data available.');
}
});
&lt;/script></description></item></channel></rss>