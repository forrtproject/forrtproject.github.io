&lt;h2 id="-by-day">&amp;hellip; by day&lt;/h2>
&lt;canvas id="visitorsChart" width="400" height="200">&lt;/canvas>
&lt;script src="https://cdn.jsdelivr.net/npm/chart.js">&lt;/script>
&lt;script>
document.addEventListener('DOMContentLoaded', function() {
var dailyData = [{"date":"2025-07-31","users":146},{"date":"2025-08-05","users":145},{"date":"2025-08-13","users":145},{"date":"2025-07-22","users":133},{"date":"2025-08-06","users":123},{"date":"2025-08-07","users":119},{"date":"2025-08-11","users":117},{"date":"2025-07-24","users":116},{"date":"2025-08-04","users":116},{"date":"2025-08-12","users":116},{"date":"2025-08-14","users":108},{"date":"2025-07-21","users":104},{"date":"2025-08-01","users":104},{"date":"2025-07-28","users":103},{"date":"2025-07-23","users":101},{"date":"2025-08-19","users":97},{"date":"2025-07-30","users":93},{"date":"2025-08-18","users":90},{"date":"2025-08-02","users":89},{"date":"2025-08-08","users":85},{"date":"2025-07-26","users":82},{"date":"2025-07-29","users":79},{"date":"2025-07-25","users":78},{"date":"2025-08-15","users":78},{"date":"2025-08-17","users":71},{"date":"2025-08-20","users":60},{"date":"2025-08-03","users":54},{"date":"2025-08-10","users":53},{"date":"2025-07-27","users":49},{"date":"2025-08-09","users":49},{"date":"2025-08-16","users":47}];
if (dailyData &amp;&amp; dailyData.length) {
dailyData.sort((a, b) => new Date(a.date) - new Date(b.date));
dailyData.pop();
var labels = dailyData.map(function(entry) { return entry.date; });
var dataPoints = dailyData.map(function(entry) { return entry.users; });
var ctx = document.getElementById('visitorsChart').getContext('2d');
new Chart(ctx, {
type: 'line',
data: {
labels: labels,
datasets: [{
label: 'Daily Visitors',
data: dataPoints,
borderColor: 'rgba(75, 192, 192, 1)',
backgroundColor: 'rgba(75, 192, 192, 0.2)',
fill: true
}]
},
options: {
responsive: true,
scales: {
x: {
title: { display: true, text: 'Date' }
},
y: {
title: { display: true, text: 'Users' }
}
}
}
});
} else {
console.warn('No daily visitor data available.');
}
});
&lt;/script></description></item></channel></rss>