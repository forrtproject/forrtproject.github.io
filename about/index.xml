&lt;h2 id="last-30-days">Last 30 Days&lt;/h2>
&lt;h3 id="-by-country">&amp;hellip; by country&lt;/h3>
&lt;div id="ga_map" style="height: 400px; background: #b3d9ff;">&lt;/div>
&lt;script>
document.addEventListener('DOMContentLoaded', function() {
console.log("Initializing map...");
if (window.myLeafletMap) {
console.warn("Map already exists! Removing it first...");
window.myLeafletMap.remove();
delete window.myLeafletMap;
}
console.log("Creating new Leaflet map...");
window.myLeafletMap = L.map('ga_map', {
zoomControl: false,
scrollWheelZoom: false,
dragging: true,
doubleClickZoom: false,
touchZoom: false,
maxBoundsViscosity: 1.0
}).setView([40, 0], 1.25);
var gaRegionsData = [{"country":"United States of America","users":1247},{"country":"United Kingdom","users":892},{"country":"Germany","users":654},{"country":"Netherlands","users":423},{"country":"Canada","users":387},{"country":"Australia","users":298},{"country":"France","users":234},{"country":"Switzerland","users":187},{"country":"Italy","users":156},{"country":"Spain","users":134},{"country":"Sweden","users":123},{"country":"Belgium","users":98},{"country":"Austria","users":87},{"country":"Norway","users":76},{"country":"Denmark","users":65},{"country":"Finland","users":54},{"country":"Ireland","users":43},{"country":"Portugal","users":38},{"country":"New Zealand","users":32},{"country":"Poland","users":28}];
var gaRegions = {};
gaRegionsData.forEach(function(entry) {
gaRegions[entry.country] = entry.users;
});
function getColor(d) {
return d > 1000 ? '#004529' :
d > 500 ? '#005824' :
d > 200 ? '#006837' :
d > 100 ? '#238443' :
d > 50 ? '#41ab5d' :
d > 20 ? '#78c679' :
d > 10 ? '#addd8e' :
d > 1 ? '#d9f0a3' :
d === 1 ? '#ffffcc' : '#f0f0f0';
}
function style(feature) {
var countryName = feature.properties.name;
var users = gaRegions[countryName] || 0;
return {
fillColor: getColor(users),
weight: 1,
opacity: 1,
color: 'white',
dashArray: '3',
fillOpacity: 0.7
};
}
fetch('/countries.geojson')
.then(response => response.json())
.then(geojsonData => {
console.log("GeoJSON Loaded, adding to map...");
L.geoJson(geojsonData, {
style: style,
onEachFeature: function(feature, layer) {
var countryName = feature.properties.name;
var users = gaRegions[countryName] || 0;
layer.bindPopup(countryName + ": " + users + " users");
}
}).addTo(window.myLeafletMap);
console.log("GeoJSON correctly added to the map.");
setTimeout(() => {
window.myLeafletMap.invalidateSize();
}, 500);
})
.catch(error => {
console.error("Error loading GeoJSON:", error);
});
});
&lt;/script>
