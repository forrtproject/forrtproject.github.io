&lt;h2 id="-by-day">&amp;hellip; by day&lt;/h2>
&lt;canvas id="visitorsChart" width="400" height="200">&lt;/canvas>
&lt;script src="https://cdn.jsdelivr.net/npm/chart.js">&lt;/script>
&lt;script>
document.addEventListener('DOMContentLoaded', function() {
var dailyData = [{"date":"2025-03-17","users":234},{"date":"2025-03-20","users":170},{"date":"2025-03-05","users":158},{"date":"2025-03-18","users":150},{"date":"2025-02-27","users":148},{"date":"2025-03-14","users":147},{"date":"2025-03-04","users":134},{"date":"2025-03-19","users":134},{"date":"2025-03-07","users":133},{"date":"2025-03-13","users":131},{"date":"2025-03-21","users":129},{"date":"2025-02-28","users":128},{"date":"2025-02-24","users":123},{"date":"2025-03-12","users":120},{"date":"2025-03-06","users":118},{"date":"2025-03-11","users":117},{"date":"2025-02-25","users":116},{"date":"2025-03-15","users":110},{"date":"2025-03-10","users":109},{"date":"2025-02-26","users":103},{"date":"2025-03-03","users":95},{"date":"2025-03-16","users":87},{"date":"2025-03-22","users":84},{"date":"2025-02-23","users":69},{"date":"2025-02-22","users":64},{"date":"2025-03-01","users":60},{"date":"2025-03-02","users":60},{"date":"2025-03-09","users":58},{"date":"2025-03-08","users":57},{"date":"2025-03-23","users":31},{"date":"2025-03-24","users":3}];
if (dailyData &amp;&amp; dailyData.length) {
dailyData.sort((a, b) => new Date(a.date) - new Date(b.date));
dailyData.pop();
var labels = dailyData.map(function(entry) { return entry.date; });
var dataPoints = dailyData.map(function(entry) { return entry.users; });
var ctx = document.getElementById('visitorsChart').getContext('2d');
new Chart(ctx, {
type: 'line',
data: {
labels: labels,
datasets: [{
label: 'Daily Visitors',
data: dataPoints,
borderColor: 'rgba(75, 192, 192, 1)',
backgroundColor: 'rgba(75, 192, 192, 0.2)',
fill: true
}]
},
options: {
responsive: true,
scales: {
x: {
title: { display: true, text: 'Date' }
},
y: {
title: { display: true, text: 'Users' }
}
}
}
});
} else {
console.warn('No daily visitor data available.');
}
});
&lt;/script></description></item></channel></rss>