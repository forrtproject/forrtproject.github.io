&lt;h2 id="-by-country">&amp;hellip; by country&lt;/h2>
&lt;div id="ga_map" style="height: 400px; background: #b3d9ff;">&lt;/div>
&lt;script>
document.addEventListener('DOMContentLoaded', function() {
console.log("Initializing map...");
if (window.myLeafletMap) {
console.warn("Map already exists! Removing it first...");
window.myLeafletMap.remove();
delete window.myLeafletMap;
}
console.log("Creating new Leaflet map...");
window.myLeafletMap = L.map('ga_map', {
zoomControl: false,
scrollWheelZoom: false,
dragging: true,
doubleClickZoom: false,
touchZoom: false,
maxBoundsViscosity: 1.0
}).setView([40, 0], 1.25);
var gaRegionsData = [{"country":"United States of America","users":740},{"country":"Germany","users":432},{"country":"United Kingdom","users":410},{"country":"Netherlands","users":255},{"country":"Ireland","users":120},{"country":"Canada","users":102},{"country":"India","users":62},{"country":"Australia","users":57},{"country":"Switzerland","users":57},{"country":"China","users":53},{"country":"Austria","users":51},{"country":"France","users":46},{"country":"Egypt","users":42},{"country":"Italy","users":38},{"country":"Finland","users":34},{"country":"Norway","users":34},{"country":"Saudi Arabia","users":31},{"country":"Brazil","users":30},{"country":"Spain","users":28},{"country":"Philippines","users":25},{"country":"Sweden","users":25},{"country":"Algeria","users":23},{"country":"Belgium","users":16},{"country":"Japan","users":16},{"country":"Poland","users":16},{"country":"South Africa","users":15},{"country":"New Zealand","users":14},{"country":"Denmark","users":13},{"country":"Hong Kong","users":13},{"country":"Croatia","users":12},{"country":"Türkiye","users":12},{"country":"Iraq","users":11},{"country":"United Arab Emirates","users":11},{"country":"Hungary","users":10},{"country":"Israel","users":10},{"country":"Kenya","users":10},{"country":"Morocco","users":10},{"country":"Greece","users":9},{"country":"Portugal","users":9},{"country":"Czechia","users":8},{"country":"Malaysia","users":8},{"country":"Nigeria","users":7},{"country":"Russia","users":7},{"country":"South Korea","users":7},{"country":"Yemen","users":7},{"country":"Indonesia","users":6},{"country":"Singapore","users":6},{"country":"Colombia","users":5},{"country":"Mexico","users":5},{"country":"Serbia","users":5},{"country":"Taiwan","users":5},{"country":"(not set)","users":4},{"country":"Argentina","users":4},{"country":"Romania","users":4},{"country":"Tunisia","users":4},{"country":"Cyprus","users":3},{"country":"Iran","users":3},{"country":"Jordan","users":3},{"country":"Kuwait","users":3},{"country":"Latvia","users":3},{"country":"Libya","users":3},{"country":"Oman","users":3},{"country":"Slovenia","users":3},{"country":"Tanzania","users":3},{"country":"Thailand","users":3},{"country":"Uganda","users":3},{"country":"Zimbabwe","users":3},{"country":"Bulgaria","users":2},{"country":"Ethiopia","users":2},{"country":"Lebanon","users":2},{"country":"Lithuania","users":2},{"country":"Luxembourg","users":2},{"country":"Malawi","users":2},{"country":"Nepal","users":2},{"country":"Pakistan","users":2},{"country":"Palestine","users":2},{"country":"Sudan","users":2},{"country":"Armenia","users":1},{"country":"Bangladesh","users":1},{"country":"Cambodia","users":1},{"country":"Chile","users":1},{"country":"Congo - Kinshasa","users":1},{"country":"Dominican Republic","users":1},{"country":"Estonia","users":1},{"country":"Iceland","users":1},{"country":"Martinique","users":1},{"country":"Mauritania","users":1},{"country":"Montenegro","users":1},{"country":"Peru","users":1},{"country":"Rwanda","users":1},{"country":"Senegal","users":1},{"country":"Sierra Leone","users":1},{"country":"Slovakia","users":1},{"country":"South Sudan","users":1},{"country":"Syria","users":1},{"country":"Togo","users":1},{"country":"Ukraine","users":1},{"country":"Uruguay","users":1}];
var gaRegions = {};
gaRegionsData.forEach(function(entry) {
gaRegions[entry.country] = entry.users;
});
function getColor(d) {
return d > 1000 ? '#004529' :
d > 500 ? '#005824' :
d > 200 ? '#006837' :
d > 100 ? '#238443' :
d > 50 ? '#41ab5d' :
d > 20 ? '#78c679' :
d > 10 ? '#addd8e' :
d > 1 ? '#d9f0a3' :
d === 1 ? '#ffffcc' : '#f0f0f0';
}
function style(feature) {
var countryName = feature.properties.name;
var users = gaRegions[countryName] || 0;
return {
fillColor: getColor(users),
weight: 1,
opacity: 1,
color: 'white',
dashArray: '3',
fillOpacity: 0.7
};
}
fetch('/countries.geojson')
.then(response => response.json())
.then(geojsonData => {
console.log("GeoJSON Loaded, adding to map...");
L.geoJson(geojsonData, {
style: style,
onEachFeature: function(feature, layer) {
var countryName = feature.properties.name;
var users = gaRegions[countryName] || 0;
layer.bindPopup(countryName + ": " + users + " users");
}
}).addTo(window.myLeafletMap);
console.log("GeoJSON correctly added to the map.");
setTimeout(() => {
window.myLeafletMap.invalidateSize();
}, 500);
})
.catch(error => {
console.error("Error loading GeoJSON:", error);
});
});
&lt;/script>
